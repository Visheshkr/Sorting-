// Bubble Sort

#include<iostream>
using namespace std;
void Bubblesort(int a[],int n){
    int temp;
    for(int i=0;i<n;i++){
        for(int j=0;j<n-i-1;j++){
        if(a[j]>a[j+1]){
        temp=a[j];
        a[j]=a[j+1];
        a[j+1]=temp;        
        }
    }
    
}
}

void printArray(int a[],int n){
    for(int i=0;i<n;i++){
        cout<<a[i]<<"  ";
    }
}
int main(){
    int size;
    cout<<"\n Enter the size of the array"<<endl;
    cin>>size;
    int a[size];
    for(int i=0;i<size;i++)
    cin>>a[i];
    Bubblesort(a,size);
    printArray(a,size);
    return 0;
}
