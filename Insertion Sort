// Insertion Sort

#include<iostream>
using namespace std;
void Insertionsort(int a[],int n){
    int temp,key,j;
    for(int i=0;i<n;i++){                                  
        key=a[i];
        j=i-1;
        while(j>=0&&a[j]>key){
            a[j+1]=a[j];
            j=j-1;
        }
        a[j+1]=key;    
}
}

void printArray(int a[],int n){
    for(int i=0;i<n;i++){
        cout<<a[i]<<"  ";
    }
}
int main(){
    int size;
    cout<<"\n Enter the size of the array"<<endl;
    cin>>size;
    int a[size];
    for(int i=0;i<size;i++)
    cin>>a[i];
    Insertionsort(a,size);
    printArray(a,size);
    return 0;
}
