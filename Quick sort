#include<iostream>
using namespace std;
int partition(int *a,int start,int end){
    int pivot=a[start],temp;
    int i=start+1;
    int j=end;
    do{
        while(a[i]<=pivot){
            i++;
        }
        while(a[j]>pivot){
            j--;
        }
        if(i<j){
            temp=a[i];
            a[i]=a[j];
            a[j]=temp;
        }
    }while(i<j);
        temp=a[start];
        a[start]=a[j];
        a[j]=temp;
    return j;  
    }

void quicksort(int *a,int start,int end){
    
    if(start<end){
        int pivot=partition(a,start,end);
        quicksort(a,start,pivot-1);
        quicksort(a,pivot+1,end);
    }
}

void printArray(int *a,int end){
    cout<<"\n Array elements are"<<endl;
    for(int i=0;i<end;i++)
    cout<<a[i]<<" ";
}

int main(){
    int n;
    cin>>n;
    int a[n];
    cout<<"\n Enter values in the array"<<endl;
    for(int i=0;i<n;i++)
    cin>>a[i];
    printArray(a,n);
    quicksort(a,0,n);
    printArray(a,n);
    return 0;
    
}
