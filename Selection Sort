// Selection Sort

#include<iostream>
using namespace std;
void Selectionsort(int a[],int n){
    int min,temp,index=0;
    for(int i=0;i<n-1;i++){
        min=i;
        for(int j=i+1;j<n;j++){
        if(a[j]<a[min]){
            min=j;
        }
        }
    temp=a[i];
    a[i]=a[min];
    a[min]=temp;
}
}

void printArray(int a[],int n){
    for(int i=0;i<n;i++){
        cout<<a[i]<<"  ";
    }
}
int main(){
    int size;
    cout<<"\n Enter the size of the array"<<endl;
    cin>>size;
    int a[size];
    for(int i=0;i<size;i++)
    cin>>a[i];
    Selectionsort(a,size);
    printArray(a,size);
    return 0;
}
